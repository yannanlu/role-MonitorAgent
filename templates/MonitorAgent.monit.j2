check process MonitorAgent
    matching "-DAgent "
    start program = "/usr/bin/systemctl start MonitorAgent"
    stop program = "/usr/bin/systemctl stop MonitorAgent"
    if does not exist 5 cycles then restart
    if cpu usage > {{cpu_pct}}% for 5 cycles then alert
    if memory usage > {{mem_pct}}% for 5 cycles then alert
    if 3 restarts within 5 cycles then timeout
